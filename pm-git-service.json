{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {
        "name": "pm-git-services",
        "annotations": {
            "description": "Service definitions for Product Machine Git."
        }
    },
    "items": [
        {
            "kind": "Service",
            "apiVersion": "v1",
            "metadata": {
                "name": "pm-git-service"
            },
            "spec": {
                "ports": [
                    {
                        "name": "pmrs4repo",
                        "protocol": "TCP",
                        "port": 9090,
                        "targetPort": 9090
                    }
                ],
                "selector": {
                    "app": "git"
                },
                "clusterIP": "172.30.252.206",
                "type": "ClusterIP",
                "sessionAffinity": "None"
            }
        }
    ]
}
