{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {
        "name": "pm-git-services",
        "annotations": {
            "description": "Service definitions for Product Machine Git."
        }
    },
    "items": [
{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "pm-git-service"
    },
    "spec": {
        "selector": {
            "app": "git"
        },
        "ports": [
            {
                "name": "pmrs4repo",
                "protocol": "TCP",
                "port": 9090,
                "targetPort": 9090
            }
        ]
    }
}
    ]
}
