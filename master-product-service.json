{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {
        "name": "master-product-services",
        "annotations": {
            "description": "Service definitions for Product Machine Master Product."
        }
    },
    "items": [
        {
            "kind": "Service",
            "apiVersion": "v1",
            "metadata": {
                "name": "master-product-service"
            },
            "spec": {
                "ports": [
                    {
                        "name": "mp-node-1",
                        "protocol": "TCP",
                        "port": 5401,
                        "targetPort": 5401
                    },
                     {
                        "name": "mp-node-2",
                        "protocol": "TCP",
                        "port": 5402,
                        "targetPort": 5402
                    },
                    {
                        "name": "mp-root",
                        "protocol": "TCP",
                        "port": 9400,
                        "targetPort": 9400
                    }
                ],
                "clusterIP": "172.30.208.206",
                "type": "ClusterIP",
                "sessionAffinity": "None",
                "selector": {
                    "app": "master-product"
                }
            }
        }
    ]
}
